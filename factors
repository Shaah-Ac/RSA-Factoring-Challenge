#!/usr/bin/python3

import sys
import time


def factorize(num):
    """Factorizes a number into two smaller factors, or returns None if prime."""
    # Implement factorization logic here
    # ...


if __name__ == "__main__":
    """Main function to handle file input, factorization, and output."""

    # Check for correct usage
    if len(sys.argv) != 2:
        print("Usage: factors <file>")
        exit()

    # Read input file
    input_file = sys.argv[1]
    try:
        with open(input_file, "r") as f:
            lines = f.readlines()
    except FileNotFoundError:
        print("File not found:", input_file)
        exit()

    # Factorize numbers and print results within time limit
    start_time = time.time()
    for line in lines:
        num = int(line.strip())
        factors = factorize(num)
        if factors:
            print(f"{num}={factors[0]}*{factors[1]}")
        else:
            print(f"{num} is prime")

        if time.time() - start_time > 5:
            print("Time limit exceeded")
            exit()

